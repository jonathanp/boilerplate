{
  "name": "boilerplate",
  "title": "Boilerplate",
  "version": "0.1.0",
  "private": true,
  "main": "lib/server.js",
  "author": "Jonathan Persson <persson.jonathan@gmail.com>",
  "scripts": {
    "build": "webpack --progress --colors",
    "build:prod": "npm run build -- --optimize-minimize",
    "dev": "NODE_ENV=development nodemon --ignore src/components",
    "start": "npm run build && node lib/server.js",
    "styleguide:dev": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=4.0"
  },
  "dependencies": {
    "express": "4.13.4",
    "express-react-views": "0.10.0",
    "react": "0.14.7",
    "react-dom": "0.14.7"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "6.6.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "glob": "7.0.0",
    "import-glob-loader": "1.1.0",
    "node-sass": "3.4.2",
    "nodemon": "1.9.1",
    "postcss-loader": "0.8.1",
    "react-styleguidist": "2.0.1",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.9.1"
  }
}
