{
  "name": "boilerplate",
  "title": "Boilerplate",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "author": "Jonathan Persson <persson.jonathan@gmail.com>",
  "scripts": {
    "build": "webpack --progress --colors",
    "build:prod": "npm run build -- --optimize-minimize",
    "dev": "NODE_ENV=development nodemon --ignore src/components",
    "start": "npm run build && node server.js",
    "styleguide:dev": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=4.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "express-react-views": "^0.9.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "6.0.4",
    "node-sass": "^3.4.2",
    "nodemon": "1.8.1",
    "postcss-loader": "^0.8.0",
    "react-styleguidist": "1.3.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "1.12.12",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.6.4"
  }
}
